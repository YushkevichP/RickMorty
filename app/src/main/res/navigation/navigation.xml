        android:name="com.example.hm7_cleanarchitecture.fragments.SettingsFragment"
        tools:layout="@layout/fragment_settings" />

    <fragment
        android:id="@+id/navigation_map"
       android:name="com.example.hm7_cleanarchitecture.googlemap.MapFragment"
        tools:layout="@layout/fragment_settings" />

